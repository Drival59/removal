{% extends 'base.html.twig' %}

{% block body %}
    <br>
<div class="all_form_raid">
    <div class="form_raid">
        {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_label(form.name) }}
        {{ form_widget(form.name) }}
        {{ form_label(form.inProgress) }}
        {{ form_widget(form.inProgress) }}
        <div class="raid_submit">
        {{ form_rest(form) }}
        </div>
        {{ form_end(form) }}
    </div>
</div>

{% endblock %}